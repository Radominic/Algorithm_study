    "def dp(n):\n",
    "    mem = [0 for _ in range(n+2)]\n",
    "    for i in range(n+2):\n",
    "        if i==0:\n",
    "            mem[0] =0\n",
    "        elif i==1:\n",
    "            mem[1] =1\n",
    "        else:\n",
    "            mem[i] = mem[i-1] + mem[i-2]\n",
    "    return mem[-1]\n"
   ]
